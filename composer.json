{
    "name": "dalholm/laravel-vuetify-spa-admin",
    "description": "A simple lightweight admin template based on laravel and vuetifyjs. Simple and clean",
    "keywords": ["spa", "laravel", "vue", "admin", "template", "frontend", "Material design", "vuetifyjs", "vuetify", "Boilerplate"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Mikael Dalholm",
            "email": "mikael@dalholm.se",
            "homepage": "http://www.dalholm.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "fideloper/proxy": "^4.2",
        "laravel/airlock": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel/tinker": "^2.2",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "laravel/dusk"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "compile": [
            "php artisan clear-compiled && php artisan optimize",
            "php artisan migrate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
